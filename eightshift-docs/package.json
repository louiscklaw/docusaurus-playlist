{
  "name": "@eightshift/docs",
  "version": "1.0.0",
  "description": "A documentation website for Eightshift open source projects.",
  "author": {
    "name": "Eightshift team",
    "email": "team@eightshift.com",
    "url": "https://eightshift.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/eightshift-docs.git"
  },
  "keywords": [
    "frontend",
    "scss",
